{
  "name": "@boiler/common",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "tsx --watch src/index.ts",
    "build:decl": "tsc --build tsconfig.build.json",
    "build:src": "tsx ./bundle.ts",
    "build": "npm run build:decl && npm run build:src",
    "start": "node dist/index.js"
  }
}
